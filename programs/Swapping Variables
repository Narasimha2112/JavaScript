var x = 45;
var y = 78;

document.writeln("Before swapping: x = " + x + " y = " +y + "<br>");
// Swapping logic using a temporary variable
temp = x;
x = y;
y = temp;

document.writeln("After swapping: x = " + x + " y = " +y + "<br>");

// Swapping back without using a temporary variable
x = x + y;
y = x - y;
x = x - y;

document.writeln("After swapping back: x = " + x + " y = " +y + "<br>");
